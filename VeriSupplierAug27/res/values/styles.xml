<resources>

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="Theme.AppCompat.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>

    <style name="animations_popup">
        <item name="@android:windowEnterAnimation">@anim/in_from_left</item>
        <item name="@android:windowExitAnimation">@anim/out_to_left</item>
    </style>

    <style name="animations_popup_new">
        <item name="@android:windowEnterAnimation">@anim/in_from_left</item>
        <item name="@android:windowExitAnimation">@anim/out_to_left</item>
    </style>

    <style name="animations_rightpopup">
        <item name="@android:windowEnterAnimation">@anim/in_from_right</item>
        <item name="@android:windowExitAnimation">@anim/out_to_right</item>
    </style>

    <style name="TwoWayView">
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="alertanimation_popup">
        <item name="@android:windowEnterAnimation">@anim/slide_down</item>
        <item name="@android:windowExitAnimation">@anim/slide_up</item>
    </style>

    <style name="MyListBackContent">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">80dp</item>
    </style>

    <style name="MyListFrontContent">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">80dp</item>
    </style>
    
    <style name="MyListImage">
        <item name="android:layout_width">10dp</item>
        <item name="android:layout_height">78dp</item>
        <item name="android:scaleGravity">fill</item>
        
    </style>

</resources>